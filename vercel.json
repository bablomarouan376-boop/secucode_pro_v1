{
  "version": 2,
  "name": "tareq-secu-pro-final",
  "builds": [
    {
      "src": "app.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/robots.txt",
      "dest": "/robots.txt"
    },
    {
      "src": "/sitemap.xml",
      "dest": "/sitemap.xml"
    },
    {
      "src": "/sw.js",
      "dest": "/sw.js"
    },
    {
      "src": "/manifest.json",
      "dest": "/manifest.json"
    },
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "app.py"
    }
  ],
  "env": {
    "FIREBASE_CREDENTIALS": "@firebase_credentials",
    "VIRUSTOTAL_API_KEY": "@virustotal_api_key",
    "TELEGRAM_TOKEN": "@telegram_token",
    "CHAT_ID": "@chat_id"
  }
}
